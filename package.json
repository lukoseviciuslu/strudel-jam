{
  "name": "strudel-jam",
  "version": "1.0.0",
  "description": "A public benchmark gallery for Strudel music generation with LLMs",
  "scripts": {
    "bench": "python scripts/run_bench.py --only-new",
    "serve": "python -m http.server 8000 -d public",
    "generate-pages": "python scripts/generate_pages.py"
  },
  "keywords": [
    "strudel",
    "music",
    "ai",
    "benchmark",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@strudel/core": "^1.2.2",
    "@strudel/mini": "^1.2.2",
    "@strudel/tonal": "^1.2.2",
    "@strudel/web": "^1.2.3"
  },
  "notes": {
    "strudel-embed": "@strudel/embed is loaded from CDN in browser (unpkg.com)"
  }
}
